 1 file changed, 3 deletions(-)

diff --git a/recipe/meta.yaml b/recipe/meta.yaml
index 3ea7189..dd7e9cb 100644
--- a/recipe/meta.yaml
+++ b/recipe/meta.yaml
@@ -23,12 +23,9 @@ outputs:
       ignore_run_exports:
         - libgcc-ng
         - libstdcxx-ng
-        - _openmp_mutex
       run_exports:
         strong:
           - {{ pin_subpackage("llvm-openmp", max_pin=None) }}
-          - _openmp_mutex >={{ openmp_ver }}  # [linux]
-          - _openmp_mutex * *_llvm            # [linux]
     requirements:
       build:
         - {{ compiler('cxx') }}  # [unix]
